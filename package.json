{
  "name": "anandchowdhary.com",
  "version": "5.0.0",
  "repository": "git@github.com:AnandChowdhary/anandchowdhary.com",
  "author": "Anand Chowdhary <mail@anandchowdhary.com>",
  "license": "MIT",
  "scripts": {
    "build-site": "rm -rf life-data && git clone --depth 1 https://github.com/AnandChowdhary/life-data && yarn build && node helpers/sitemap.js",
    "copy-highlights": "mkdirp public/images/highlights && cp -r life-data/highlights public/images",
    "copy-cache-images": "mkdirp public/images/cache && cp -r .cache/images public/images/cache",
    "build": "node helpers/life-data.js && node helpers/blog-tags.js && eleventy && gulp && yarn copy-highlights",
    "serve": "npm run gulp && eleventy --serve & gulp watch",
    "gulp": "gulp"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@babel/core": "^7.8.3",
    "axios": "^0.19.2",
    "axios-cache-adapter": "^2.5.0",
    "del": "^5.1.0",
    "front-matter": "^3.1.0",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "js-yaml": "^3.13.1",
    "recursive-readdir": "^2.2.2",
    "slugify": "^1.3.6"
  },
  "dependencies": {
    "download": "^7.1.0"
  }
}
